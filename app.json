{
  "expo": {
    "name": "walletApp",
    "slug": "walletApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/avatar.png",
    "splash": {
      "image": "./images/logo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#483d8b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Lleva el control total de tus finanzas desde la palma de tu mano. Darte cuenta de los gastos que realizas te ayudara a pensar 2 veces antes de sacar la tarjeta para pagar.",
    "githubUrl": "https://github.com/RivaldoEsteban/WalletApp-reactNative"
  }
}
